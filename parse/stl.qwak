function Bell() {
  s = |00>;
  cx = control(X);
  H(s)[0];
  cx(s);
  return s
}

function KetPlus() {
  x = |0>;
  return H(x)
}

function Deutsch(U) {
  x=|01>;
  hh = H^*H;
  hh(x);
  U(x);
  hh(x);
  return measure(x)[0]
}

function Entangle(n) {
  nn = n - 1;
  xx = X^**nn;
  cx = control(xx);
  x = |0^n>;
  H(x)[0];
  cx(x);
  return x
}